{
  "name": "ember-routing-utils",
  "version": "0.1.0",
  "description": "Utility tools for working with the Ember `RouterService`",
  "keywords": [
    "ember-addon",
    "routing",
    "router",
    "util",
    "helper",
    "utils",
    "helpers",
    "url",
    "parameters",
    "params"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:buschtoens/ember-routing-utils.git"
  },
  "license": "MIT",
  "author": "Jan Buscht√∂ns <jan@buschtoens.me> (https://jan.buschtoens.me)",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:types": "tsc --noEmit",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "prettier": "@clark/prettier-config",
  "dependencies": {
    "ember-cli-babel": "^7.21.0",
    "ember-cli-typescript": "^4.0.0"
  },
  "devDependencies": {
    "@clark/ember-template-lint-config": "^0.2.2",
    "@clark/eslint-config-ember": "^1.24.5",
    "@clark/eslint-config-ember-typescript": "^1.24.5",
    "@clark/eslint-config-node": "^1.24.5",
    "@clark/prettier-config": "^1.28.2",
    "@ember/edition-utils": "^1.2.0",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "~1.0.1",
    "@glimmer/tracking": "~1.0.1",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.9",
    "@types/ember-resolver": "^5.0.9",
    "@types/ember__test-helpers": "^1.7.2",
    "@types/qunit": "^2.9.1",
    "@types/rsvp": "^4.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.21.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "1.1.3",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.12.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.9.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0",
    "release-it": "^14.0.2",
    "release-it-lerna-changelog": "^2.4.0",
    "typescript": "~4.0.2"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "14.10.1"
  }
}
